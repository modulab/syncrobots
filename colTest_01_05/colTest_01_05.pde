        catch (NumberFormatException e) {        
          println("err y");
        }

      }


      if (data[indexRobot].indexOf("O") == 0) {
        try {
        dir[indexRobot] = Integer.valueOf(data[indexRobot].substring(1));
        dir[indexRobot] = dir[indexRobot] +60;
        dir[indexRobot] = (dir[indexRobot] / 6 +180) % 360;
        }
        catch (NumberFormatException e) {        
          println("err o");
        }

      }
  } 
  }

}


